{
    "rules": {
        ".read": true,
        ".write": true,
        "users": {
            "$uid": {
                ".read": "$uid === auth.uid",
                ".write": "$uid === auth.uid"
            },
            ".indexOn": ["uid"]
        },
        "walls": {
            ".indexOn": ["gym_id", "id", "uid"]
        },
        "routes": {
            ".indexOn": ["gym_id", "wall_id", "id"]
        },
        "sent_routes": {
            ".indexOn": ["gym_id", "route_id", "user_id", "id"]
        }
    }
}
